- var path = is_development ? '/public' : '/act-overlay-kyaputen/public'
head
    title Kyaputen
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1, shrink-to-fit=no")
    //- Stylesheets
    link(rel="stylesheet", href="https://fonts.googleapis.com/css?family=Oxygen")
    link(rel="stylesheet", href=`${path}/stylesheets/style.css`)
    //- Javascript
    script(type="application/javascript", integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=", crossorigin="anonymous", src="https://code.jquery.com/jquery-3.4.1.min.js", defer)
    script.
      window.jQuery || document.write(`<script type="application/javascript" src='#{path}/javascripts/dist/jquery/jquery.min.js' defer><\/script>`)
    //- Libraries
    script(type="application/javascript", src=`${path}/javascripts/dist/bootstrap/bootstrap.bundle.min.js`, defer)
    script(type="application/javascript", src=`${path}/javascripts/dist/mustache/mustache.min.js`, defer)
    script(type="application/javascript", src=`${path}/javascripts/dist/papaparse/papaparse.min.js`, defer)
    //- Global variables
    script(type="application/javascript").
      const path = "#{path}"
    //- Scripts
    script(type="application/javascript", src=`${path}/javascripts/utils.js`, defer)
    script(type="application/javascript", src=`${path}/javascripts/ws.js`, defer)
    //- Dashboard
    script(type="application/javascript", src=`${path}/javascripts/dashboard.js`, defer)